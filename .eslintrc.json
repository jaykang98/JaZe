{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
    "plugins": [
      "react",
      "@typescript-eslint"
    ],
    "rules": {
    },
    "overrides": [
      {
        "files": [ "**/*.ts", "**/*.tsx" ],
        "rules": {
          "react/prop-types": "off"
        }
      }
    ]
  }
