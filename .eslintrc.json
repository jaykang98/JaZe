{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard-with-typescript",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
  },
  "overrides": [
    {
      "files": [ "*.js", "*.jsx", "*.ts", "*.tsx" ],
      "extends": "standard-with-typescript"
    }
  ]
}
